<div class="date-container">
  <h2 class="date"><%= date %></h2>
</div>
<br>
<div class="stats-header">
  <h2><%= link_to "View machine accuracy information", counselor_machine_path, class:'form-button' %></h2>
</div>
<br>
<div class="banner">
  <div class="dynamic-container">
    <div class="dynamic">
      <section class='flagged-strategies'>
        <h3>Strategies we've flagged as "potentially unhelpful":</h3>
        <% facade.flagged_strats.each do |strat| %>
        <section class='strategy'>
          <p><%= strat.strategy %></p>
          <%= button_to 'Delete this strategy', deactivate_strategy_path(strat), method: :patch %>
          <%= button_to "Approve this strategy", approve_strategy_path(strat), method: :patch %>
        </section>
        <% end %>
      </section>
    </div>
  </div>
</div>
